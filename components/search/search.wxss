.searchBar{
  width: 100%;
  display: flex;
  
}

.searchBar .section{
  width: 100%;
  position: relative;
  margin: 32rpx 30rpx 0 32rpx;
  height: auto;
}
.search_img{
position: absolute;
left: 30rpx;
top: 15rpx;
}
.searchBar .section .remove{
  height: 30rpx;
  width: 30rpx;
  position: absolute;
  top: 13rpx;
  right: 30rpx;
  z-index: 999;
}

.searchBar .section .input-placeholder{
   margin: 1rpx; 
}
.searchBar .section icon {
  position: absolute;
  left: 32rpx;
  bottom: 10rpx;
  
  z-index: 99;
}
.searchBar .section input{
  border-radius: 8rpx;
  width: 100%;
  height: 56rpx;
  background: #F1F1F1;
  padding-left: 78rpx;
  padding-right: 60rpx;
  display: block;
  box-sizing: border-box;
  font-size:26rpx;
}

