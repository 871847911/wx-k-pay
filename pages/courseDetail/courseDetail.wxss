/* bottom */

.course-bottom {
  height: 100rpx;
  border-top: 1px #eee solid;
  width: 100%;
  display: flex;
  justify-content: center;
  align-content: center;
  position: fixed;
  bottom: 0;
  left: 0;
  background: white;
}

.course-bottom .sold-out {
  
  width: 750rpx;
  height: 98rpx;
  background: #6CDDC7;
  border-radius: 4rpx;
  position: absolute;
  bottom: 4rpx;
  color: white;
}

.course-bottom .course-bottom-item {
  padding-top: 18rpx;
  width: 138rpx;
  display: flex;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
}

.course-bottom .course-bottom-item button::after {
  border: none;
}

.course-bottom .course-bottom-item button {
  margin: 0;
  padding: 0;
  background: none;
  height: auto;
}

.course-bottom .course-bottom-item icon, .course-bottom .course-bottom-item text {
  display: flex;
  width: 100%;
  justify-content: center;
  align-content: center;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  color: rgba(102, 102, 102, 1);
  line-height: 40rpx;
}

