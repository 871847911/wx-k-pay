/* pages/hasSettled/hasSettled.wxss *//* pages/income/income.wxss */
.container{
  padding-left: 30rpx;
  padding-right: 30rpx;
  font-size: 28rpx;
  font-weight:400;
color:rgba(72,72,72,1);
}
.tab{ 
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width:100% ;

  
}
.tab_title{
  width: 375rpx;
  text-align: center;
  line-height: 100rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  
}
.tab_title_text{
  text-align: center;
  line-height: 100rpx;
  height: 100%;
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1); 
  
  
}
.change{
  font-size:36rpx;
  color: #6CDDC7;

}
.line{
  width:48rpx;
height:4rpx;
background:rgba(108,221,199,1);
border-radius:4rpx;

}
.income{
  padding-top: 16rpx;
  border-bottom: 1rpx solid rgba(241,241,241,1);
  display: flex;
  flex-direction: column;
  padding-bottom: 40rpx;
 
}
.whobuy{
  display: flex;
  flex-direction: row;
}
.class_name{
  width: 516rpx;
}
.name_price{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.date_service{
  margin-top: 16rpx;
  font-size:24rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(153,153,153,1);
  display: flex;
  justify-content: space-between;
}

