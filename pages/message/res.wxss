
.resPage-title{
  width: 100%;
  line-height: 40rpx;
  padding-left: 32rpx;
  /* padding: 20rpx; */
  display: flex;
  flex-wrap: wrap;
  border-bottom: 2rpx solid #F1F1F1;
  margin-bottom: 32rpx; 
}

.course{
  box-sizing: border-box; 
  width: 100%;
  line-height: 40rpx;
  padding-top: 10rpx;
  display: block;
}
 
.course .course-title {
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between ;
  padding:0 32rpx;
  height: 60rpx;
  font-size: 44rpx;
  color: #484848;
  line-height: 60rpx;
}
.course .course-title .more{
   height: 40rpx;
  font-size: 28rpx;
  color: #484848;
  font-weight: 500;
}

.course view{
  box-sizing: border-box;
}

.resPage-title_css{
  display: flex;
  flex-direction: column;
  align-items: center;

}
.resPage-title .resPage-title-item{
  /* width: 33.33%; */
  
  margin-right: 32rpx;
  padding-bottom: 22rpx!important;
  text-align: center;
  padding: 10rpx 0;
  color: #999999;
  font-size: 30rpx;
}

.resPage-title  .active{

  font-size: 36rpx;
  color: #6cddc7;
  font-weight: 400;
}
.line{
   margin-right: 32rpx;
  width:40rpx;
height:4rpx;
background:#6cddc7;
border-radius:4px;

}
.empty-content {
    width: 300rpx;
    height: 186rpx;
    margin: 302rpx auto;
    margin-bottom: 50rpx;
}

.empty-content image {
    width: 100%;
    height: 100%;
}

.empty-content text {
    display: block;
    font-family: PingFangSC-Regular;
    color: rgba(153, 153, 153, 1);
    line-height: 42px;
    text-align: center;
}

.border-top-241 {
    border-top: 1rpx solid rgba(241, 241, 241, 1);
}