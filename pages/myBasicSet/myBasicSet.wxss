.my-header {
  height: 128rpx;
  display: flex;
  margin: 0 30rpx 20rpx 32rpx;
}

.my-header .name {
  width: calc(100% - 128rpx);
  height: 128rpx;
}

.my-header .img {
  width: 128rpx;
  height: 128rpx;
  border: 2rpx solid #f1f1f1;
  border-radius: 50%;
}

.my-header image {
  width: 100%;
  height: 100%;
  border: 2rpx solid #f1f1f1;
  border-radius: 50%;
}

.my-header .name .username {
  height: 96rpx;
  font-size: 48rpx;
  font-family: PingFangSC-Medium;
  color: rgba(72, 72, 72, 1);
  line-height: 96rpx;
  font-weight: 400;
}

.my .myset-item2 {
  border-bottom: 2rpx solid #f1f1f1;
  margin-left: 32rpx;
  margin-right: 30rpx;
  padding-top: 28rpx;
  padding-bottom: 28rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  color: rgba(102, 102, 102, 1);
  line-height: 50rpx;
  display: flex !important;
  width: calc(100% - 62rpx) !important;
  justify-content: space-between;
}

.my .myset-item2 .des {
  min-width: 180rpx;
}

.my .myset-item2 .img {
  height: 44rpx;
  font-size: 30rpx !important;
  font-family: PingFangSC-Regular;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
  margin-top: -5rpx;
}

.popupBackground {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.4);
  z-index: 199;
}

.popup {
  width: 688rpx;
  height: 512rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 28rpx;
  position: absolute;
  left: 50%;
  margin-left: -344rpx;
  top: 50%;
  margin-top: -256rpx;
  z-index: 200;
  overflow: hidden;
}

.popupTitle {
  font-size: 36rpx;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
  text-align: center;
  padding: 50rpx 0 36rpx 0;
}

.inputBox {
  width: 608rpx;
  height: 84rpx;
  border-radius: 8rpx;
  border: 2rpx solid rgba(212, 212, 212, 1);
  margin: 0 auto;
  overflow: hidden;
}

.codeBox {
  width: 608rpx;
  height: 88rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  margin-top: 52rpx;
}

.codeInput {
  width: 352rpx;
  height: 84rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  border: 2rpx solid rgba(212, 212, 212, 1);
  font-size: 32rpx;
  padding-left: 24rpx;
}
.picker{
  width:100%;
}

.phoneInput {
  padding-left: 24rpx;
  width: 608rpx;
  height: 84rpx;
  background: rgba(255, 255, 255, 1);
  font-size: 32rpx;
}

.codeBtn {
  width: 208rpx;
  height: 88rpx;
  background: rgba(108, 221, 199, 1);
  border-radius: 8rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.popupBtn {
  position: absolute;
  bottom: 0;
  height: 88rpx;
  width: 100%;
  border-top: 1rpx solid rgba(212, 212, 212, 1);
  display: flex;
  justify-content: space-between;
}

.popupBtn view {
  width: 343rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 36rpx;
  font-weight: 400;
}

.popupCancel {
  color: rgba(51, 51, 51, 1);
  border-right: 1rpx solid rgba(212, 212, 212, 1);
}

.popupDetermine {
  color: rgba(108, 221, 199, 1);
}

.active {
  background: rgba(212, 212, 212, 1);
}
picker-view{position: relative}
.pickerView{
  position: absolute ;
  height: 20rpx;background: red;width: 100%
}