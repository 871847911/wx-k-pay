/* pages/myGroupList/myGroupList.wxss */
page{
  overflow: hidden;
}
.resPage-title {
  width: 100%;
  line-height: 40rpx;
  padding-left: 32rpx;
  /* padding: 20rpx; */
  display: flex;
  flex-wrap: wrap;
  border-bottom: 2rpx solid #f1f1f1;
}

.course {
  box-sizing: border-box;
  width: 100%;
  line-height: 40rpx;
  padding-top: 10rpx;
  display: block;
}

.course .course-title {
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 0 32rpx;
  height: 60rpx;
  font-size: 44rpx;
  color: #484848;
  line-height: 60rpx;
}

.course .course-title .more {
  height: 40rpx;
  font-size: 28rpx;
  color: #484848;
  font-weight: 500;
}

.course view {
  box-sizing: border-box;
}

.resPage-title .resPage-title-item {
margin-right: 48rpx;
  text-align: center;
  color: #999999;
  font-size: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.resPage-title  .active {
  
  font-size: 36rpx;
  color: #6CDDC7;
  font-weight: 800;
}

.gropListBox {
  border-bottom: 1rpx solid rgba(241, 241, 241, 1);
  padding: 33rpx 32rpx;
}

.gropList {
  display: flex;
}

.courseUrl {
}

.courseTitle {
  width: 410rpx;
  height: 72rpx;
  line-height: 36rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(17, 17, 17, 1);
  padding: 0 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.courseUrl image {
  width: 160rpx;
  height: 160rpx;
}

.price {
  flex: 1;
  text-align: right;
}

.gropPirce {
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(72, 72, 72, 1);
  padding-bottom: 12rpx;
}

.oldPrice {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-decoration: line-through;
  padding-bottom: 12rpx;
}

.tuikuan {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(254, 128, 96, 1);
}

.groupListBtn {
  display: flex;
  justify-content: space-between;
}

.groupListBtn2 {
  overflow: hidden;
}

.groupListBtn .textLeft {
  line-height: 64rpx;
  color: #fe8060;
}

.shareBtn {
  width: 244rpx;
  height: 64rpx;
  background: #6CDDC7;
  border-radius: 8rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 64rpx;
  text-align: center;
}

.textLeftNone {
  float: left;
  line-height: 64rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.deletBtn {
  float: right;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  border: 2rpx solid rgba(208, 208, 208, 1);
  width: 192rpx;
  height: 64rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 64rpx;
  text-align: center;
  margin-right: 32rpx;
}

.lookBtn {
  float: right;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  border: 2rpx solid #6CDDC7;
  width: 192rpx;
  height: 64rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: #6CDDC7;
  line-height: 64rpx;
  text-align: center;
}
.line{
   margin-top: 26rpx;
  display: flex;
  align-items: center;
  background: #6CDDC7;
  width: 40rpx;
  height: 4rpx;
}