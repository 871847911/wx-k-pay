/* pages/requestWithdrawal/requestWithdrawal.wxss */

.container {
  box-sizing: border-box;
  padding-right: 30rpx;
  padding-left: 30rpx;
}

.chosesBank {
  border-bottom: 1rpx solid rgba(241, 241, 241, 1);
  height: 124rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(72, 72, 72, 1);
}

.right {
  height: 20rpx;
  width: 20rpx;
}

.bank {
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
}

.bankContent {
  margin-left: 40rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(72, 72, 72, 1);
}
.money_all{
  padding-top: 30rpx;
  height: 210rpx;
  border-bottom: 1rpx solid rgba(241, 241, 241, 1);
  font-size:30rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(153,153,153,1);
}
.withdrawal{
  line-height: 42rpx;
}
.withdrawal_num{
  height: 134rpx;
  font-size:68rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(102,102,102,1);
}

.money {
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  border-bottom: 1rpx solid rgba(241, 241, 241, 1);
}

.money_text {
  margin-top: 40rpx;
  display: flex;
  flex-direction: column;
  height: 40rpx;
}

.money_text_num {
  margin-left: 40rpx;
}
.money_getAll{
  margin-top: 10rpx;
  margin-bottom: 40rpx;
display: flex;
flex-direction: row;
align-items: center;
justify-content: space-between;

}

.money_num {

  font-size:30rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(200,200,200,1);
  display: flex;
  flex-direction: row;
  line-height: 40rpx;
  text-align: center;
}
.num {
  margin-left: 6rpx;
  
}
.getAll{
font-size:30rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:#6CDDC7;
}
.more_money{
  font-size:30rpx;
font-family:PingFangSC-Regular;
font-weight:400;
  margin-top: 10rpx;
  line-height: 40rpx;
  margin-bottom: 40rpx;
  color: #FF9595;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}



.commit_bt {
  margin-top: 104rpx;
  width: 100%;
  height: 98rpx;
  background: #6CDDC7;
  border-radius: 4px;
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}
.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
.drawer_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding-left: 32rpx;
  padding-right: 32rpx;
  font-size:30rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(72,72,72,1);
}
.title{
  height: 100rpx;
  width: 100%;
  line-height: 100rpx;
  text-align: center;
  font-size:36rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(51,51,51,1);
display: flex;
align-items: center;
justify-content: space-around;
border-bottom: 1rpx solid rgba(241,241,241,1);
}
.bankMsg{
  height: 120rpx;
  width: 100%;
  
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1rpx solid rgba(241,241,241,1);
}
.newBank{
  height: 120rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid rgba(241,241,241,1);
}